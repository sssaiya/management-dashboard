{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Management Dash",
    "description": "Your portal into team productivity",
    "icons": {
        "16": "images/puppy16.png",
        "48": "images/puppy48.png",
        "128": "images/puppy.png"
    },
    "chrome_url_overrides": {
        "newtab": "dashboard.html"
    },
    "browser_action": {
        "default_icon": "images/puppy.png"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}